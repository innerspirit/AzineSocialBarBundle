{
    "name": "innerspirit/socialbar-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to easily add a social-bar to your pages, updated for Symfony 3 compatibility",
    "keywords": ["social bar", "social", "share", "share button", "like", "like button"],
    "homepage": "https://github.com/innerspirit/AzineSocialBarBundle",
    "license": "MIT",
    "authors": [
	{
            "name": "Christian Maioli Mackeprang",
            "email": "chris@chrismm.com"
        },
        {
            "name": "Azine IT Services AG",
            "email": "github@azine-it.ch"
        }
    ],
    "config": {
        "process-timeout": 590,
        "github-protocols": ["https", "git", "ssh"],
        "github-domains": ["github.com"]
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.7",
        "symfony/symfony": ">=2.7",
        "twig/extensions": "~1.0",
        "phpunit/phpunit":  "~5.0"
    },
    "autoload": {
        "psr-0": { "Azine\\SocialBarBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Azine/SocialBarBundle"
}
